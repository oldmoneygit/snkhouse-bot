{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd ../.. && pnpm install --frozen-lockfile && pnpm --filter @snkhouse/widget build",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@snkhouse_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@snkhouse_supabase_anon_key",
    "SUPABASE_SERVICE_ROLE_KEY": "@snkhouse_supabase_service_role_key",
    "ANTHROPIC_API_KEY": "@snkhouse_anthropic_api_key",
    "OPENAI_API_KEY": "@snkhouse_openai_api_key",
    "WOOCOMMERCE_URL": "@snkhouse_woocommerce_url",
    "WOOCOMMERCE_CONSUMER_KEY": "@snkhouse_woocommerce_consumer_key",
    "WOOCOMMERCE_CONSUMER_SECRET": "@snkhouse_woocommerce_consumer_secret"
  }
}
